{
  "urdf_robot": {
    "prefix": "robot",
    "body": [
      "<robot>",
      "  $0",
      "</robot>"
    ],
    "description": "URDF robot tag"
  },
  "urdf_link": {
    "prefix": "link",
    "body": [
      "<link name=\"$1\">",
      "  $0",
      "</link>"
    ],
    "description": "URDF link tag"
  },
  "urdf_joint": {
    "prefix": "joint",
    "body": [
      "<joint name=\"$1\" type=\"$2\">",
      "  $0",
      "</joint>"
    ],
    "description": "URDF joint tag"
  },
  "urdf_origin": {
    "prefix": "origin",
    "body": [
      "<origin xyz=\"$1 0 0\" rpy=\"$2 0 0\"/>"
    ],
    "description": "URDF origin tag"
  },
  "urdf_axis": {
    "prefix": "axis",
    "body": [
      "<axis xyz=\"$1 0 0\"/>"
    ],
    "description": "URDF axis tag"
  },
